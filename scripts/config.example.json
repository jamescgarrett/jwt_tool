{
  "custom": {
    "use_rs": true,
    "debug": false,
    "claims": {
      "iss": "<ISS_CLAIM>",
      "aud": "<AUD_CLAIM>",
      "client_id": "<CLIENT_ID_CLAIM>",
      "sub": "<SUB_CLAIM>",
      "scope": "<SCOPE_CLAIM>",
      "urn:auth0:connection": "<CUSTOM_CLAIM>",
      "urn:auth0:identity_user_id": "<CUSTOM_CLAIM>"
    },
    "header": {
      "kid": "<JWK_KID>"
    },
    "jwk_local": false,
    "jwk_local_file": "<LOCAL_JWK_FILE_IF_jwk_local",
    "well_known_endpoint": "<WELL_KNOWN_ENDPOINT>",
    "private_key_file_path": "<PEM_PRIVATE_KEY_FILE>"
  },
  "rs": {
    "setup_rs": false,
    "domain": "<DOMAIN i.e. tenant.local.dev.auth.com>",
    "client_secret": "<CLIENT_SECRET>",
    "client_id": "<CLIENT_ID>",
    "username": "<AUTH0 User Identifier>",
    "password": "<Auth0 User Password>"
  }
}
